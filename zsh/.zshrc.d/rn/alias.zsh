#[ Profile ]
alias ezsh='code ~/.zshrc'
alias szsh='source ~/.zshrc && clear'

#[ Base ]
alias ls='ls -GaFh'
alias ll='ls -lGa'

#[ Directory ]
alias desktop='cd ~/desktop'
alias work='cd ~/Work'
alias home='cd ~'

#[ Logging ]
alias gitlog="git log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit"
